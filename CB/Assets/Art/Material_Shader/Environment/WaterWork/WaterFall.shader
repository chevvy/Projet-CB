				float simplePerlin2D16 = snoise( panner17*5.0 );
				simplePerlin2D16 = simplePerlin2D16*0.5 + 0.5;
				float simplePerlin2D21 = snoise( panner17*7.0 );
				simplePerlin2D21 = simplePerlin2D21*0.5 + 0.5;
